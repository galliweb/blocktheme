{
	"$schema": "https://schemas.wp.org/wp/6.9/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"blocks": {
			"core/heading": {
				"color": {
					"custom": true
				}
			}
		},
		"border": {
			"color": false,
			"radius": false,
			"style": false,
			"width": false
		},
		"color": {
			"custom": false,
			"customGradient": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"gradients": [],
			"palette": [
				{
					"color": "#1a1a1a",
					"name": "Schwarz",
					"slug": "schwarz"
				},
				{
					"color": "#ff6b6b",
					"name": "Coral",
					"slug": "coral"
				},
				{
					"color": "#ffffff",
					"name": "Weiss",
					"slug": "weiss"
				}
			]
		},
		"layout": {
			"allowCustomContentAndWideSize": true,
			"allowEditing": true,
			"contentSize": "840px",
			"wideSize": "1280px"
		},
		"spacing": {
			"customSpacingSize": false,
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "XXS (8px)",
					"size": "8px",
					"slug": "xxs"
				},
				{
					"name": "XS (16px)",
					"size": "16px",
					"slug": "xs"
				},
				{
					"name": "SM",
					"size": "24px",
					"slug": "sm"
				},
				{
					"name": "MD",
					"size": "32px",
					"slug": "md"
				},
				{
					"name": "LG",
					"size": "48px",
					"slug": "lg"
				},
				{
					"name": "XL",
					"size": "64px",
					"slug": "xl"
				},
				{
					"name": "XXL",
					"size": "80px",
					"slug": "xxl"
				},
				{
					"name": "3XL",
					"size": "104px",
					"slug": "3-xl"
				},
				{
					"name": "4XL",
					"size": "128px",
					"slug": "4-xl"
				},
				{
					"name": "5XL",
					"size": "160px",
					"slug": "5-xl"
				}
			],
			"units": [
				"px"
			]
		},
		"typography": {
			"customFontSize": false,
			"defaultFontSizes": false,
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/inter/inter-100-900-normal.woff2"
							]
						}
					],
					"fontFamily": "Inter",
					"name": "Inter",
					"slug": "inter"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"max": "0.875rem",
						"min": "0.75rem"
					},
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1rem",
						"min": "0.875rem"
					},
					"name": "Medium",
					"size": "1rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "1.25rem",
						"min": "1rem"
					},
					"name": "Large",
					"size": "1.25rem",
					"slug": "large"
				}
			],
			"fontStyle": false,
			"fontWeight": false,
			"letterSpacing": false,
			"lineHeight": false,
			"textDecoration": false,
			"textTransform": false
		},
		"useRootPaddingAwareAlignments": false
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--weiss)",
			"text": "var(--wp--preset--color--schwarz)"
		},
		"elements": {
			"h1": {
				"typography": {
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"lineHeight": "1.2"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--coral)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontStyle": "normal",
					"fontWeight": "900",
					"lineHeight": "1.2"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--schwarz)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--coral)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)"
				}
			}
		},
		"spacing": {
			"padding": {
				"bottom": "0",
				"left": "1rem",
				"right": "1rem",
				"top": "0"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--inter)",
			"fontSize": "1.125rem",
			"fontStyle": "normal",
			"fontWeight": "400",
			"lineHeight": "1.6"
		}
	}
}